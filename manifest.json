{
    "manifest_version": 3,
    "name": "ELI",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon.png"
    },
    "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    },
    "content_scripts": [{
      "matches": ["https://*.medium.com/*"],
      "js": ["content.js"]
    }]
  }
  